# API Configuration
VITE_API_BASE_URL=http://localhost:3000

# AWS Configuration
VITE_S3_BUCKET_NAME=your-bucket-name
VITE_AWS_REGION=us-east-1

# Application Configuration
VITE_NODE_ENV=development
VITE_ENABLE_DEBUG_LOGGING=false

# Development Notes:
